    <validationRules>
        <fullName>Capacity_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Capacity__c  &lt; 0 || Capacity__c  &gt; 100000</errorConditionFormula>
        <errorDisplayField>Capacity__c</errorDisplayField>
        <errorMessage>Value must be between 0 and 100,000</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Location_Name</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
            Name =  VLOOKUP( $ObjectType.VEM_Location__c.Fields.Name,
                                                  $ObjectType.VEM_Location__c.Fields.Name, Name),
           NOT(Id = VLOOKUP( $ObjectType.VEM_Location__c.Fields.Id,
                                                  $ObjectType.VEM_Location__c.Fields.Name, Name))

 )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Location already exists</errorMessage>
    </validationRules>
